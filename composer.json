{
    "name": "dyce/laravel-bouncer",
    "description": "Laravel Bouncer provides a featherweight authentication system for gaming APIs built with Laravel.",
    "keywords": ["dyce", "bouncer", "auth", "laravel"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dycesystems/laravel-bouncer/issues",
        "source": "https://github.com/dycesystems/laravel-bouncer"
    },
    "authors": [
        {
            "name": "DYCE",
            "email": "dev@dycesystems.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.2",
        "ext-json": "*",
        "illuminate/contracts": "^6.9|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^6.9|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.9|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Dyce\\LaravelBouncer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dyce\\LaravelBouncer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dyce\\LaravelBouncer\\LaravelBouncerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
